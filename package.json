{
  "name": "portyr-client",
  "version": "0.1.0",
  "description": "Portyr Client Application",
  "main": "src/index.tsx",
  "private": true,
  "author": "Sebastian Nemeth",
  "homepage": "http://portyr.com",
  "repository": "https://github.com/portyr/portyr-client.git",
  "bugs": "https://github.com/portyr/portyr-client/issues",
  "license": "MIT",
  "scripts": {
    "ss": "node scripts/ss.js",
    "clean": "rm -rf node_modules/ dev/ .fusebox",
    "reinstall": "rm -rf node_modules/ && yarn install",
    "dev": "ts-node -P scripts/ scripts/dev.ts",
    "dist": "ts-node -P scripts/ scripts/dist.ts",
    "lib": "rm -rf lib/ && tsc --project src/ -t 'es5' -m 'commonjs'",
    "tsc": "tsc --project src/ --noEmit",
    "tsc:watch": "tsc --project src/ --noEmit -w",
    "lint": "tslint --project src/tsconfig.json",
    "test": "jest --config jest.json",
    "test:nocache": "jest --config jest.json --no-cache",
    "test:update": "jest --config jest.json -u",
    "deploy:init": "git clone https://github.com/piotrwitek/react-currency-converter --branch gh-pages dist",
    "deploy": "git checkout gh-pages && cd dist && git add --all && git commit -m \"New Release\" && git push"
  },
  "dependencies": {
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-redux": "next",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "reselect": "^3.0.0",
    "rxjs": "^5.3.0",
    "servicestack-client": "^0.0.34",
    "tslib": "^1.6.0",
    "typescript-collections": "^1.2.3",
    "typescript-fsa": "^2.3.0",
    "typescript-fsa-reducers": "^0.4.3",
    "typestyle": "^1.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/history": "^4.5.1",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.66",
    "@types/material-ui": "^0.17.11",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/react-router": "^4.0.8",
    "@types/react-router-redux": "^5.0.1",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/redux": "^3.6.0",
    "@types/shelljs": "^0.7.0",
    "express": "^4.15.2",
    "express-http-proxy": "^1.0.2",
    "fuse-box": "^2.1.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "postcss-modules": "^0.7.0",
    "shelljs": "^0.7.7",
    "ts-jest": "^19.0.8",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "tslint-react": "3.0.0",
    "typescript": "^2.3.3",
    "uglify-js": "^3.0.19"
  }
}
